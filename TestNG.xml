			<class name="com.gencare.home.negative.HomeTest"/>
			<class name="com.gencare.groups.negative.GroupsNegativeTest"/>
			<class name="com.gencare.rulegroup.negative.RuleGroupsNegativeTest" />
			<class name="com.gencare.alerts.negative.AlertsNegativeTest" />
			<class
				name="com.gencare.dashboardattributes.negative.DashboardAttributesTest" />
			<class
			name="com.gencare.notificationtoken.negative.tests.NotificationTokensNegativeTests"
			/>
			   <class name="com.gencare.devices.negative.DevicesNegativeTest"/>
			<class name="com.gencare.permissions.negative.PermissionsNegative" />

		</classes>
	</test>
</suite>
